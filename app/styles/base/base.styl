html, body 
    position relative
    padding 0
    margin 0
    background #efece7
    font-family $font
    font-size 12px
    color #656D78
    cursor default
    //min-width 1263px
    height  100%
    

/* PAGE CONTAINER */
.b-page-container
    width 100%
    float left
    position relative
    background #474b53
    display table

.b-sidebar
    width 231px
    float left
    position fixed
    top 0
    z-index 3
    
.b-inner-container
    max-width 1689px

.b-page-content
    min-height 100%
    margin-left 231px
    background #efece7 left top repeat
    position relative
    height 100%
    clearfix()

