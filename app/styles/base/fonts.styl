/* Ruble Sign */
@font-face
	font-family 'ALSRubl-Tahoma'
	font-weight normal
	font-style normal
	src url('../fonts/rubl/alsrubl-tahoma-regular.eot')
	src url('../fonts/rubl/alsrubl-tahoma-regular.eot?#iefix') format('embedded-opentype'),
		url('../fonts/rubl/alsrubl-tahoma-regular.woff') format('woff'), 
		url('../fonts/rubl/alsrubl-tahoma-regular.ttf') format('truetype'), 
		url('../fonts/rubl/alsrubl-tahoma-regular.svg#iefix') format('svg')


@font-face
	font-family 'ALSRubl-Tahoma'
	font-weight bold
	font-style normal
	src url('../fonts/rubl/alsrubl-tahoma-bold.eot')
	src url('../fonts/rubl/alsrubl-tahoma-bold.eot?#iefix') format('embedded-opentype'), url('../fonts/rubl/alsrubl-tahoma-bold.woff') format('woff'), url('../fonts/rubl/alsrubl-tahoma-bold.ttf') format('truetype'), url('../fonts/rubl/alsrubl-tahoma-bold.svg#iefix') format('svg')

@font-face {
    font-family: 'als_rublregular';
    src: url('../fonts/rubl/rouble-webfont.eot');
    src: url('../fonts/rubl/rouble-webfont.eot?#iefix') format('embedded-opentype'),
         url('../fonts/rubl/rouble-webfont.woff') format('woff'),
         url('../fonts/rubl/rouble-webfont.ttf') format('truetype'),
         url('../fonts/rubl/rouble-webfont.svg#als_rublregular') format('svg');
    font-weight: normal;
    font-style: normal;
}