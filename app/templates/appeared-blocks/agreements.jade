.b-popup-holder
    .b-popup.b-agreements-popup.j-agreements-popup
        .b-popup__close.j-modal-close
        .b-block-title__heading
            | Данные договора
        .b-popup__content
            .b-agreements-popup__details
                .b-agreements-popup__details-row
                    span.b-agreements-popup__details-text Номер договора 
                    span.g-required-filed *
                    .pkp-tooltip.pkp-tooltip_inline
                        span(title="Ключевые слова и фразы" data-position="right" data-theme="g-hint").pkp-tooltip__trigger.j-hint
                            | ?
                    strong PKP3_7
                .b-agreements-popup__details-row
                    span.b-agreements-popup__details-text Дата подписания 
                    span.g-required-filed *
                    .pkp-tooltip.pkp-tooltip_inline
                        span(title="Ключевые слова и фразы" data-position="right" data-theme="g-hint").pkp-tooltip__trigger.j-hint
                            | ?
                    strong 12.01.2015
                .b-agreements-popup__details-row
                    span.b-agreements-popup__details-text Минимальная сумма вывода 
                    span.g-required-filed *
                    .pkp-tooltip.pkp-tooltip_inline
                        span(title="Ключевые слова и фразы" data-position="right" data-theme="g-hint").pkp-tooltip__trigger.j-hint
                            | ?
                    strong 2 000 рублей

            .b-profile__agreements-days
                span.b-label
                    .b-label__text Дни вывода
                        span.g-required-filed  *
                        .pkp-tooltip.pkp-tooltip_inline
                            span(title="Ключевые слова и фразы" data-position="top" data-theme="g-hint").pkp-tooltip__trigger.j-hint
                                | ?
                    label.b-checkbox__label
                        input(type="checkbox").b-checkbox__input
                        span.b-checkbox__icon.m-checkbox__icon_big
                        span.b-ckeckbox__text.m-ckeckbox__text_big Понедельник

                    label.b-checkbox__label
                        input(type="checkbox").b-checkbox__input
                        span.b-checkbox__icon.m-checkbox__icon_big
                        span.b-ckeckbox__text.m-ckeckbox__text_big Вторник

                    label.b-checkbox__label
                        input(type="checkbox").b-checkbox__input
                        span.b-checkbox__icon.m-checkbox__icon_big
                        span.b-ckeckbox__text.m-ckeckbox__text_big Среда

                    label.b-checkbox__label
                        input(type="checkbox").b-checkbox__input
                        span.b-checkbox__icon.m-checkbox__icon_big
                        span.b-ckeckbox__text.m-ckeckbox__text_big Четверг

                    label.b-checkbox__label
                        input(type="checkbox").b-checkbox__input
                        span.b-checkbox__icon.m-checkbox__icon_big
                        span.b-ckeckbox__text.m-ckeckbox__text_big Пятница
            .b-profile__half
                label.b-label
                    .b-label__text Статус договора
                        span.g-required-filed  *
                        .pkp-tooltip.pkp-tooltip_inline
                            span(title="Ключевые слова и фразы" data-position="right" data-theme="g-hint").pkp-tooltip__trigger.j-hint
                                | ?
                    .b-select-holder
                        select.b-select.j-select
                            option
                                | Российская Федерация
                            option
                                | Запрещен
                            option
                                | Продается
                            option
                                | Вне каталога
                            option
                                | Ожидает модерации

        .b-outer-btns-holder
